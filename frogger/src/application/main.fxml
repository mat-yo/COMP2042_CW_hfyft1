<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="800.0" prefWidth="600.0" styleClass="menubackground" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.mainControl">
   <children>
   
	  <ImageView fitHeight="300.0" fitWidth="400.0" layoutX="100.0" layoutY="80.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../resources/title/gametitle.png" />
         </image>
      </ImageView>
   
   	  <Button layoutX="220.0" layoutY="380.0" mnemonicParsing="false" onAction="#game" prefHeight="40.0" prefWidth="160.0" styleClass="menubutton" text="Start Game" />
      <Button layoutX="220.0" layoutY="470.0" mnemonicParsing="false" onAction="#tutorial" prefHeight="40.0" prefWidth="160.0" styleClass="menubutton" text="How To Play" />
      <Button layoutX="220.0" layoutY="650.0" mnemonicParsing="false" onAction="#exit" prefHeight="40.0" prefWidth="160.0" styleClass="menubutton" text="Exit" />
      <Button layoutX="220.0" layoutY="560.0" mnemonicParsing="false" onAction="#score" prefHeight="40.0" prefWidth="160.0" styleClass="menubutton" text="High Scores" />
      
   </children>
</Pane>
